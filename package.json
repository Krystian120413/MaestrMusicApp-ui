{
  "name": "maestr3-ui",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "concurrently --names \"next,scss\" --prefix-colors \"magenta,yellow\" \"yarn next:dev\" \"yarn build:scss\"",
    "next:dev": "next dev",
    "build": "next build",
    "start": "node server",
    "lint:next": "eslint \"**/*.{ts,tsx}\" --cache",
    "lint:scss": "stylelint \"**/*.{css,scss}\" --cache",
    "lint": "yarn lint:next",
    "lint:fix": "yarn lint:next --fix && yarn lint:scss --fix",
    "build:scss": "typed-scss-modules core/components pages -w --additionalData \"@import 'core/styles/_mixins';\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.0",
    "@svgr/webpack": "^6.2.1",
    "@types/react-slick": "^0.23.8",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "express": "^4.18.1",
    "next": "12.1.6",
    "next-translate": "^1.4.0",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.33.0",
    "sass": "^1.52.3",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@types/node": "17.0.38",
    "@types/react": "18.0.10",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "concurrently": "^7.2.1",
    "eslint": "8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.2.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.14",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.6.1",
    "stylelint": "^14.8.5",
    "stylelint-config-css-modules": "^4.1.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "typed-scss-modules": "^6.4.1",
    "typescript": "4.7.2"
  }
}
